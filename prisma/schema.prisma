// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SERVICO {
  id Int @id @default(autoincrement())
  nome String
  duracao DateTime
}

model CLIENTE {
  id Int @id @default(autoincrement())
  nome String
  telefone String @db.VarChar(11)
}
